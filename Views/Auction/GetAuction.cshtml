@model Auction

@{
    ViewData["Title"] = "Details of the Auction";
}

<div class="row">
    <div class="col-md-8 offset-2">
        <p>
            <h1>Details for @Model.AuctionName</h1>
        </p>
        <div>
            <label>@Html.DisplayFor(auction => auction.IdAuction)</label>
            <label>@Html.DisplayFor(auction => auction.AuctionName)</label>
            <label>@Html.DisplayFor(auction => auction.IdApplicationUser)</label>
        </div>
    </div>
</div>